{
  "name": "dwt-code-generator",
  "displayName": "Angular Snippets for Dynamic Web TWAIN",
  "description": "A collection of quickstart samples and Angular code snippets demonstrating the Dynamic Web TWAIN APIs ",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "publisher": "yushulx",
  "icon": "res/dwt.jpg",
  "activationEvents": [
    "onCommand:dwt.quickstart",
    "onCommand:dwt.doc",
    "onCommand:dwt.demo",
    "onCommand:dwt.about",
    "onCommand:dwt.license"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dwt.quickstart",
        "title": "dwt: Create Sample Project"
      },
      {
        "command": "dwt.doc",
        "title": "dwt: API Reference"
      },
      {
        "command": "dwt.demo",
        "title": "dwt: Online Demo"
      },
      {
        "command": "dwt.about",
        "title": "dwt: About Dynamic Web TWAIN"
      },
      {
        "command": "dwt.license",
        "title": "dwt: Get a 30-day Free Trial License"
      }
    ],
    "snippets": [
			{
				"language": "typescript",
				"path": "./snippets/typescript.json"
			}
		]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.72.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "ts-loader": "^9.4.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@vscode/test-electron": "^2.1.5"
  },
  "author": "yushulx",
  "license": "MIT"
}
